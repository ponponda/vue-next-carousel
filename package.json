        "@testing-library/jest-dom": "^5.15.0",
        "@types/jest": "^27.0.2",
        "@vitejs/plugin-vue": "^1.9.3",
        "@vue/test-utils": "^2.0.0-rc.16",
        "babel-jest": "^27.3.1",
        "cypress": "^9.0.0",
        "jest": "^27.3.1",
        "regenerator-runtime": "^0.13.9",
        "sass": "^1.43.4",
        "ts-jest": "^27.0.7",
        "typescript": "^4.4.3",
        "vite": "^2.6.4",
        "vue-jest": "^5.0.0-alpha.10",
        "vue-tsc": "^0.3.0"
