{
    "name": "vue-next-carousel",
    "version": "1.0.0",
    "description": "Carousel Component for Vue 3",
    "author": "Burke Yu",
    "scripts": {
        "serve": "vue-cli-service serve dev/serve.ts",
        "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
        "test:unit": "vue-cli-service test:unit",
        "test:e2e": "vue-cli-service test:e2e",
        "build:doc": "vue-cli-service build dev/serve.ts --dest docs --mode development",
        "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
        "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
        "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
        "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
        "prebuild": "rimraf ./dist"
    },
    "main": "dist/vue-next-carousel.ssr.js",
    "module": "dist/vue-next-carousel.esm.js",
    "browser": "dist/vue-next-carousel.esm.js",
    "unpkg": "dist/vue-next-carousel.min.js",
    "files": [
        "dist/*",
        "src/*"
    ],
    "dependencies": {
        "core-js": "^3.16.2",
        "resize-observer-polyfill": "^1.5.1",
        "vue": "^3.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@rollup/plugin-alias": "^3.1.2",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/jest": "^24.0.19",
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-plugin-e2e-cypress": "~4.5.0",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-plugin-unit-jest": "~4.5.0",
        "@vue/cli-service": "^4.5.13",
        "@vue/compiler-sfc": "^3.0.11",
        "@vue/test-utils": "^2.0.0-0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "autoprefixer": "^10.3.1",
        "cross-env": "^7.0.3",
        "minimist": "^1.2.5",
        "node-sass": "^4.12.0",
        "postcss": "^8.3.6",
        "rimraf": "^3.0.2",
        "rollup": "^2.52.8",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-vue": "^6.0.0",
        "sass-loader": "^8.0.2",
        "ttypescript": "^1.5.12",
        "typescript": "^4.0.3",
        "vue-jest": "^5.0.0-0"
    },
    "peerDependencies": {
        "vue": "^3.0.5"
    },
    "bugs": {
        "url": "https://github.com/ponponda/vue-next-carousel/issues"
    },
    "engines": {
        "node": ">=12"
    },
    "homepage": "https://ponponda.github.io/vue-next-carousel/",
    "keywords": [
        "vue",
        "vue 3",
        "vuejs",
        "typescript",
        "carousel",
        "slider",
        "responsive"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ponponda/vue-next-carousel.git"
    },
    "sideEffects": false,
    "types": "dist/types/src/entry.esm.d.ts"
}
