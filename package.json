{
    "name": "vue-next-carousel",
    "version": "1.0.1",
    "description": "Carousel Component for Vue 3",
    "author": "Burke Yu",
    "files": ["dist"],
    "main": "./dist/vue-next-carousel.umd.js",
    "module": "./dist/vue-next-carousel.es.js",
